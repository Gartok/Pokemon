{% extends "base.html.twig" %}

{% block body %}
    <!-- Title -->
    <div class="row">
        <div class="col-lg-12">
            <h3>Object dans le sac</h3>
        </div>
    </div>
    <!-- /.row -->

    <!-- Page Features -->
    <div class="row text-center">

    {% for object in objects %}
        <div class="col-md-6 col-sm-6 hero-feature">
            <div class="thumbnail">
                <div class="caption">
                    <h3>{{ object.name }}</h3>
                    <hr />
                    <div class="col-xs-6">Nombre : {{ object.quantity }}</div>
                    <div class="col-xs-6">Type : {{ object.type.name }}</div>
                    <p>
                        <a href="#" class="btn btn-primary more-info-but">Plus d'information</a>
                    </p>
                </div>
            </div>
        </div>
    {% endfor %}
{% endblock %}